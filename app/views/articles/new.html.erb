<h1>New article</h1>
<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
<%= form_for @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
    </div>
  </div>
</div>
